	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"storybook": "concurrently 'yarn:watch:*'",
		"watch:tailwind": "npx tailwindcss -i ./src/index.css -o ./src/output.css   --watch",
		"watch:storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"prepare": "husky",
		"lint": "eslint --fix=true --ext .ts,.tsx .",
		"format": "prettier --config .prettierrc 'src/**/*.ts' 'src/**/*.tsx' --write"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"plugin:storybook/recommended",
			"plugin:storybook/recommended"
		]
	},
	"lint-staged": {
		"*.{ts,tsx}": "npx prettier --write"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@storybook/addon-essentials": "^7.6.10",
		"@storybook/addon-interactions": "^7.6.10",
		"@storybook/addon-links": "^7.6.10",
